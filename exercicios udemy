 Algoritmo "dardo"

Var
t1,t2,t3:inteiro
Inicio
    escreva("Qual foi o primeiro tempo")
     leia(t1)
      escreva("Qual foi o segundo tempo")
       leia(t2)
        escreva("o terceiro tempo")
         leia(t3)
          se (t1>t2)e(t1>t3)entao
           escreva("primeiro é o maior")
            senao
             se  t2>t3 entao
              escreva("segundo maior")
               senao
                escreva(" o terceiro é maior")
                 senao
                  se (t1=t2)e(t2=t3)entao
                   escreva("os 3 numeros deu igual")
                    senao
                     se(t1=t2)entao
                      escreva( "os seguintes valores deu igual",t1,t2)
                       se t2=t3 entao
                        escreva("os valores ...",t2,"e"t3)
                         se t2=t3 entao
                         escreva("igual"t1,t3)
                         fimse
                       fimse
                     fimse
                  fimse
             fimse
          fimse
Fimalgoritmo

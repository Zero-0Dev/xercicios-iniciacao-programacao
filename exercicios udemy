Algoritmo "media-idades"

Var
 idade,soma,cont:inteiro
 media:real

Inicio
 soma<-0
 cont<-0
  escreval("Escreva as idades")
   leia (idade)
    enquanto idade>=0 faca
     soma<-soma+idade
     cont<-cont+1
  //lembra que deve ser identificado um valor inicial em somas
  //acumulativa (x<-0) vc declara um valor inicial que sera
  //substituido a cada nova soma,a msm logica no cont
  // essa logica é para resolver"calcular e imprimir a idade media..."
      leia(idade)
  fimenquanto
   se cont= 0 entao
   escreval("impossivel calcular")
   // cont=0 se refere se no inicio ja der o valor - (idade>=0)
    media<-soma/cont
     escreval("Media",media2:3)
   fimse

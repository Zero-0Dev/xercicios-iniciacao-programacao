Algoritmo "temperatura"
Var
   c, f: real
   escala: caractere
Inicio
   escreva("Qual medida de temperatura você quer ver? (F/C): ")
   leia(escala)

   se escala = "C" entao
      escreva("Digite a temperatura em Fahrenheit: ")
      leia(f)
      c <- (f - 32) * 5 / 9
      escreva("A temperatura em Celsius é: ", c:6:2)

   senao
      se escala = "F" entao
         escreva("Digite a temperatura em Celsius: ")
         leia(c)
         f <- (c * 9 / 5) + 32
         escreva("A temperatura em Fahrenheit é: ", f:6:2)
      senao
         escreva("Escala inválida. Digite apenas 'F' ou 'C'.")
      fimse
   fimse
Fimalgoritmo
 
 
 algoritimo "lanchonete"

var  
codigo,quantidade:inteiro
preco,total:inteito 

inicio 
  escreva("Qual o codigo do produto")
   leia(codigo)
    escreva("Qual a quantidade comprada?")
     leia(quantidade)
    se (codigo=1)entao 
      preco<-5*quantidade
         total<-preco 
          escreva("o total da compra é R$ ",preco)
           senao 
            se (codigo=2) entao 
             preco<-3.50*quantidade
              total<-preco 
               escreva("o total da compra é R$ ", preco)
                senao 
                 Se (codigo=3)entao 
                  preco<-4.80*quantidade
                   total<-preco 
                    escreva("o total da compra é R$ ", preco)
                     senao 
                      se (codigo=4)entao 
                       preco<-8.90*quantidade 
                       total<-preco 
                        escreva("o total da compra é R$ ", preco)
                        senao 
                         se (codigo=5)entao 
                    preco<-7.32*quantidade
                    total<-preco
                     escreva("o total da compra é R$ ", preco)      
                        
                         
                        
                      
                         fimse 
                    
                      fimse
                 fimse
            fimse 
    
    fimse
    
    
    
    

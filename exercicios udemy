Algoritmo "media-idades-v2"

Var
   idade, soma, cont : inteiro
   media            : real

Inicio
   soma <- 0
   cont <- 0

   escreval("Digite uma idade :")
   leia(idade)

   enquanto idade >= 0 faca
      soma <- soma + idade
      cont <- cont + 1
      escreval("Digite outra idade :")
      leia(idade)
   fimenquanto

   se cont = 0 entao   // essa linha so vai rodar se no inico
                       // ja for dado um numero -
      escreval("IMPOSSIVEL CALCULAR")
   senao
      media <- soma / cont     // essa so vai rodar apenas quando
                               // tiver os numeros >0 e quando
                                // finalizar quando aparecer um <
                                // que seria o negativo
      escreval("MEDIA = ", media:8:2)
   fimse
Fimalgoritmo
(melhor que o do professor)


Algoritmo "Para"

Var
  n,soma:inteiro


Inicio
  soma<-0
   escreval("Quantos numero serao digitados?")
    leia   (n)
     para n de 1 ate 3 faca
      escreva(" escreva o proximo numero")
       leia  (n)
       soma<-soma+n
     fimpara

         escreval("O valor da soma é,",soma)



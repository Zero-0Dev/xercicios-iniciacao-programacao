Algoritmo "semnome"

Var
   idade,soma,cont:inteiro
   media:real


Inicio
  soma<-0         //Inicializa a soma em 0
  cont<-0         // Inicializa o contador em 0
  
  escreval("digite as idades")
  leia(idade)
   enquanto idade>=0 faca
   soma<-soma+idade
   cont<-cont+1
  leia (idade)
   fimenquanto
  se cont=0 entao
   escreval("impossivel de calcular")
    senao
     media<-soma/cont
     escreval("media",media:8:2)
  fimse




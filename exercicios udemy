Algoritmo "nota"
// 
//  
// Descrição   : Aqui você descreve o que o programa faz! (função)
// Autor(a)    : Nome do(a) aluno(a)
// Data atual  : 5/5/2025
Var
  notaFinal ,notaR1 ,notaR2 :real

Inicio
   escreva("qual foi sua nota R1")
    leia(notaR1)
   escreva("qual foi a nota de R2")
    leia(notaR2)
      notaFinal<-(notaR1+notaR2)
        se (notaFinal>=60.00)entao
    escreva("parabens corno VOCE ESTA  aprovado")
          senao
    escreva("hahaha se fudeo REPROVADO ")
          fimse
      
Fimalgoritmo






Algoritmo "baskara"

Var
   a,b,c,delta,x1,x2:real


Inicio
   escreva("Qual o coeficiente A")
    leia (a)
     escreva("Qual o coeficiente de B")
      leia (b)
       escreva("qual o coeficiente C")
       leia(c)
 delta<-(exp(b,2)-(4*a*c))
   se (delta<0)entao
       escreva(" Deu negativo seu troxa")
   senao
       x1<-(-b+raizq(delta)\(2*a))
       x2<-(-b-raizq(delta)\(2*a))

    fimse
       escreva ("o valor de x1 é ",x1:2:3," e o valor de x2 é",x2:2:3)




Fimalgoritmo




Algoritmo "menor de tres"

Var
menor,a,b,c:inteiro

  inicio
        escreva("Digite o priemeiro numero")
        leia   (a)
        escreva("digite o segundo numero")
        leia   (b)
        escreva("Digite o terceiro numero")
        leia   (c)
  
        se (a=b)e(b=c)entao
         escreva("todos numeros são iguais")
        senao
       se (a<b)e(a<c)entao
         menor<-a
       escreva("o menor numero é o primeiro",a.)
         senao
.             se(b<c)entao
       menor<-b
                escreva("o menor numero é o segundo",b)
          senao
              menor<-c
       fimse
           fimse
              fimse

Fimalgoritmo


Algoritmo "operadora"

var
 minutos:inteiro
 valorpago:real
Inicio
  escreva("Qual foi a quantidade de tempo consumida")
  leia   (minutos)
  valorpago<-50
  
     se (minutos<=100) entao
        valorpago<-50
        escreva("o valor da fatura é R$ 50")
      senao
          se (minutos>100)entao
             valorpago<-valorpago+ 2*(minutos-100)
             escreva("o valor a se pago é: ",valorpago)
          fimse
     fimse
  
.


Fimalgoritmo




Algoritmo "troco_verificado"
var
troco,preco,dinheiro,resto,pago:real
quantidade:inteiro


 //Se o dinheiro dado pelo cliente não for suficiente,
  //mostrar uma mensagem informando o valor restante

Inicio
    escreva("Qual é o preço do produto?")
    leia   (preco)
    escreva("Qual foi a quantidade de produto comprado")
    leia   (quantidade)
    dinheiro<- preco*quantidade
    escreva("pode me informar quanto voce dara de dinheiro?")
    leia   (pago)
       se  pago<dinheiro entao
       troco<-pago-dinheiro
       escreva(" a compra deu R$",dinheiro,"falta R$",troco)
         senao
             se pago>dinheiro entao
             troco<-pago-dinheiro
               escreval("A compra deu,",dinheiro,"e seu troco é R$",troco)

             fimse
       fimse

Fimalgoritmo




Algoritmo "glicose"
/
Var
     glicose:real

Inicio
   escreva("qualé o valor dado de glicose")
   leia   (glicose)
        se glicose<=100 entao
          escreva("O valor esta dentro da normalidade")
           senao
                se (glicose>100)e (glicose<=140)entao
                escreva("A glicose esta elevada CUIDADO")
                  senao
                     se glicose>140 entao
                     escreva("diabetes ein se fudeu")

                     fimse
                fimse
        fimse
Fimalgori




Algoritmo "glicose"  corrigido
/
Var
     glicose:real

Inicio
   escreva("qualé o valor dado de glicose")
   leia   (glicose)
        se glicose<=100 entao
          escreva("O valor esta dentro da normalidade")
           senao
                se (glicose>100)e (glicose<=140)entao
                //na teoria nao precisa repitir"glicose>100
                //poderia colocar"se glicose<=140entao
                escreva("A glicose esta elevada CUIDADO")
                  senao
                     se glicose>140 entao
                     escreva("diabetes ein se fudeu")

                     fimse
                fimse
        fimse

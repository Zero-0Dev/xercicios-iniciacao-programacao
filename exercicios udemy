Algoritmo "dardo"

Var

   d1, d2, d3, maior : real

Inicio

   escreval("Digite as tres distancias:")
   leia(d1)
   leia(d2)
   leia(d3)

   se (d1 > d2) e (d1 > d3) entao
      maior <- d1
   senao
      se d2 > d3 entao
         maior <- d2
      senao
         maior <- d3
      fimse
   fimse
   
   escreval("MAIOR DISTANCIA = ", maior:6:2)

Fimalgoritmo


Algoritmo "temperatura"

Var
 c,f:real
 escala:caractere

Inicio
   escreva("Qual medida de temperatura vc quer verF/C?")
    leia(escala)
     se escala=c entao
      escreva("escreva a temperatura em C")
       leia  (c)
        f<-(c*9/5)+32
         escreva("A temperatura em F é:",f )
          senao
           se escala=f entao
            escreva("Escreva a temperatura em f")
             leia  (f)
              c<-(f-32)*5/9
                escreva(A temperatura em C é;",c)
           fimse
     fimse



Var
  N, i, x, soma: inteiro
Inicio
  escreva("Quantos números serão digitados? ")
  leia(N)
  soma <- 0

  para i de 1 ate N faca
    escreva("Digite um numero: ")
    leia(x)
    soma <- soma + x
  fimpara

  escreval("SOMA = ", soma)

Algoritmo "Crescente"

Var

   x,y:inteiro
   

Inicio
  x<-0
  y<-0

    escreval("escreva o valor de x e y")

    leia(x)
    leia(y)
     enquanto x<>y faca

      se (x<y) entao
       escreva("Esta ordem é crescente")
       senao
        se x>y entao
        escreval("Esta ordem é decrescente")

        fimse
      fimse
      escreval("escreva o valor de x e y")
        leia (x)
        leia (y)
     fimenquanto
     
  
  
.


Fimalgoritmo
